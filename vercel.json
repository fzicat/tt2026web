{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/ibkr/positions/(.*)",
      "dest": "/api/ibkr/positions.py"
    },
    {
      "src": "/api/ibkr/positions",
      "dest": "/api/ibkr/positions.py"
    },
    {
      "src": "/api/ibkr/trades/(.*)",
      "dest": "/api/ibkr/trades.py"
    },
    {
      "src": "/api/ibkr/trades",
      "dest": "/api/ibkr/trades.py"
    },
    {
      "src": "/api/ibkr/import",
      "dest": "/api/ibkr/import_trades.py"
    },
    {
      "src": "/api/ibkr/mtm",
      "dest": "/api/ibkr/mtm.py"
    },
    {
      "src": "/api/ibkr/stats/daily",
      "dest": "/api/ibkr/stats/daily.py"
    },
    {
      "src": "/api/ibkr/stats/weekly",
      "dest": "/api/ibkr/stats/weekly.py"
    },
    {
      "src": "/api/fbn/accounts",
      "dest": "/api/fbn/accounts.py"
    },
    {
      "src": "/api/fbn/entry/(.*)",
      "dest": "/api/fbn/entry.py"
    },
    {
      "src": "/api/fbn/entry",
      "dest": "/api/fbn/entry.py"
    },
    {
      "src": "/api/fbn/stats/monthly",
      "dest": "/api/fbn/stats/monthly.py"
    },
    {
      "src": "/api/fbn/stats/yearly",
      "dest": "/api/fbn/stats/yearly.py"
    },
    {
      "src": "/api/fbn/matrix/monthly",
      "dest": "/api/fbn/matrix/monthly.py"
    },
    {
      "src": "/api/fbn/matrix/yearly",
      "dest": "/api/fbn/matrix/yearly.py"
    },
    {
      "src": "/api/config",
      "dest": "/api/config.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
